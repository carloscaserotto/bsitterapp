<h1>Log in</h1>
<%= form_with(scope: :sessions, url: login_path, local: true)  do |f| %>
  <p>
  <%= f.label 'email' %><br>
  <%= f.text_field :email %>
  </p>
  <p>
  <%= f.label 'password' %><br>
  <%= f.password_field :password %>
  </p>
  <p>
  <%= f.label 'password_confirmation' %><br>
  <%= f.password_field :password_confirmation %>
  </p>
  <p>
  <%= f.submit 'Login' %>
  </p>
<% end %>

<%= link_to 'Home', root_path %>